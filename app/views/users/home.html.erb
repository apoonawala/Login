<h1>Welcome to Arif's Friend App</h1>
<h2>Please enter your login credentials</h2>

<%=form_tag user_login_url, :method => "post" do%>
<p>Email:</p><%=text_field_tag :email%>
<p>Password:</p><%=text_field_tag :password%>
<%=submit_tag "Login"%></br>
<%=link_to "Or Create an Account", signup_url%>
<%end%>
